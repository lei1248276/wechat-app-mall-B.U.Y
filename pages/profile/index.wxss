.profile_container .navbar {
  font-size: 60rpx;
  color: #fff;
  font-weight: bold;
}
.profile_container .user_info {
  width: 100vw;
  height: 45vh;
  position: relative;
  background: linear-gradient(to top, #114357, #f29492);
}
.profile_container .user_info .cell {
  width: 82%;
  position: absolute;
  bottom: 10%;
  left: 50%;
  transform: translateX(-50%);
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #fff;
}
.profile_container .user_info .cell .name {
  padding: 10rpx 0;
}
.profile_container .user_info .cell .point {
  font-size: 28rpx;
}
.profile_container .shelf {
  padding: 40rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.profile_container .custom_cell {
  padding: 40rpx 30rpx;
}
