.category_container {
  display: flex;
  justify-content: space-between;
  text-align: center;
}
.category_container .main {
  flex-grow: 1;
  height: 100vh;
  display: flex;
  justify-content: left;
  align-items: center;
  align-content: flex-start;
  flex-wrap: wrap;
}
.category_container .main .item {
  flex-basis: 28%;
  padding: 15rpx;
}
